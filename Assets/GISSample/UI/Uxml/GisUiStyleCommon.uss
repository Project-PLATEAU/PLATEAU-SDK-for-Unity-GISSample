.unity-text-element {
    color: rgb(255, 255, 255);
    -unity-font-definition: url("project://database/Assets/GISSample/Fonts/MPLUSRounded1c-Regular%20SDF.asset?fileID=11400000&guid=16ebc954c776e604e8d8693a34969fdb&type=2#MPLUSRounded1c-Regular SDF");
}

.header {
    font-size: 20px;
    -unity-font-style: bold;
    color: rgb(255, 255, 255);
    background-color: rgba(255, 255, 255, 0);
    margin: 0;
    padding: 4px 12px 6px 11px;
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    background-image: resource('img/Title_bg');
    -unity-slice-left: 10;
    -unity-slice-top: 10;
    -unity-slice-bottom: 10;
    -unity-font: initial;
    -unity-font-definition: url("project://database/Assets/GISSample/Fonts/MPLUSRounded1c-Regular%20SDF-FallbackDynamic.asset?fileID=11400000&guid=19b57f8fcf45cf94a87fb589aca3154e&type=2#MPLUSRounded1c-Regular SDF-FallbackDynamic");
    margin-bottom: 6px;
    height: 36px;
}

.gis-background-window {
    background-color: rgba(14, 21, 20, 0.7);
    margin-left: 1%;
    margin-right: 1%;
    margin-top: 1%;
    margin-bottom: 1%;
    padding-left: 16px;
    padding-right: 0;
    padding-top: 24px;
    padding-bottom: 24px;
    border-top-left-radius: 7px;
    border-bottom-left-radius: 7px;
    border-top-right-radius: 7px;
    border-bottom-right-radius: 7px;
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
}

.menu-unity-button {
    background-color: rgb(128, 213, 212);
    border-top-left-radius: 20px;
    border-bottom-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    white-space: normal;
    height: 40px;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    color: rgb(0, 55, 55);
}

.slider #unity-tracker {
    margin-top: 0;
    margin-right: 0;
