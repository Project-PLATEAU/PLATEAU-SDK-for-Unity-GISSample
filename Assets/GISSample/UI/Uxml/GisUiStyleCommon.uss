    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}

.unity-radio-button__checkmark {
    background-color: rgb(128, 213, 212);
    width: 10px;
    height: 10px;
}

.unity-radio-button__checkmark-background {
    width: 20px;
    height: 20px;
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgb(128, 213, 212);
