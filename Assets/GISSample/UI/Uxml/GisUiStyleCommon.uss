}

.unity-min-max-slider__max-thumb {
    border-radius: 4px;
    margin-top: -10px;
    background-color: rgb(235, 235, 235);
    width: 24px;
    height: 24px;
    border-top-left-radius: 12px;
    border-bottom-left-radius: 12px;
    border-top-right-radius: 12px;
