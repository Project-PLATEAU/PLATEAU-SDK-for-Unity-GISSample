Scroller #unity-dragger {
    background-color: rgb(0, 106, 106);
    display: flex;
    width: 5px;
    left: 10px;
}

Scroller #unity-tracker {
    background-color: rgba(0, 0, 0, 0);
    width: 1px;
    left: 12px;
    display: flex;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 1.5px;
}

Scroller #unity-drag-container {
    display: flex;
}

Scroller #unity-dragger-border {
}

#unity-low-button {
    background-color: rgb(79, 79, 79);
    border-top-left-radius: 2px;
    border-bottom-left-radius: 2px;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    -unity-background-image-tint-color: rgb(185, 185, 185);
    display: none;
}

#unity-high-button {
    background-color: rgb(79, 79, 79);
    border-top-left-radius: 2px;
    border-bottom-left-radius: 2px;
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
    -unity-background-image-tint-color: rgb(185, 185, 185);
    display: none;
}

.header-inner-text {
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    font-size: 20px;
}

.unity-button:hover {
    background-color: rgb(105, 149, 125);
}

.unity-button:active {
    background-color: rgb(24, 53, 33);
}

.open-close-button {
    font-size: 11px;
    height: 32px;
    right: 0;
    width: 32px;
    background-image: resource('img/Icon close');
    background-color: rgba(0, 0, 0, 0);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    top: 0;
}

.open-close-button:hover {
    background-color: rgba(0, 0, 0, 0);
    -unity-background-image-tint-color: rgb(200, 200, 200);
}

.open-close-button:active {
    -unity-background-image-tint-color: rgb(136, 147, 146);
}

.tabs {
    flex-direction: row;
    -unity-font-style: bold;
    font-size: 14px;
    margin-top: 4px;
    margin-right: 0;
    margin-bottom: 8px;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
}

.tab {
    flex-grow: 1;
    -unity-text-align: middle-center;
    background-color: rgb(80, 115, 105);
    min-height: 40px;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
}

.tab:hover {
    background-color: rgb(95, 135, 123);
}

.tab-leftmost {
    border-top-left-radius: 7px;
}

.tab-rightmost {
    border-top-right-radius: 7px;
}

.tab-contents {
    background-color: rgba(255, 255, 255, 0);
    border-left-color: rgb(111, 121, 121);
    border-right-color: rgba(111, 121, 121, 0);
    border-top-color: rgba(111, 121, 121, 0);
    border-bottom-color: rgba(111, 121, 121, 0);
    border-top-left-radius: 12px;
    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
    border-bottom-left-radius: 12px;
}

.tab-selected {
    background-color: rgb(23, 96, 58);
}

.tab-selected:hover {
    background-color: rgb(23, 96, 58);
}

.tab-content-unselected {
    display: none;
}

.menu-label {
    white-space: normal;
    font-size: 16px;
}

.unity-toggle .unity-toggle__checkmark {
    background-image: resource('img/Switch_off');
    background-color: rgba(0, 0, 0, 0);
    -unity-background-image-tint-color: rgb(255, 255, 255);
    width: 60px;
    height: 48px;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    margin-right: 12px;
}

.unity-toggle:checked .unity-toggle__checkmark {
    background-image: resource('img/Switch_on');
    -unity-background-image-tint-color: rgb(255, 255, 255);
}

.tab .unity-radio-button__checkmark-background {
    background-color: rgba(0, 0, 0, 0);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    margin-top: 4px;
    margin-right: 4px;
    margin-bottom: 4px;
    margin-left: 4px;
}

.tab .unity-radio-button__checkmark {
    background-image: resource('img/Icon_selected');
    width: 14px;
    height: 14px;
    background-color: rgba(0, 0, 0, 0);
}

.tab .unity-base-field__input {
    align-items: stretch;
    align-self: auto;
    justify-content: center;
}

.tab.left {
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgb(136, 147, 146);
    border-right-color: rgb(136, 147, 146);
    border-top-color: rgb(136, 147, 146);
    border-bottom-color: rgb(136, 147, 146);
    border-top-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-top-left-radius: 20px;
    border-bottom-left-radius: 20px;
}

.tab.right {
    background-color: rgba(0, 0, 0, 0);
    border-left-color: rgb(136, 147, 146);
    border-right-color: rgb(136, 147, 146);
    border-top-color: rgb(136, 147, 146);
    border-bottom-color: rgb(136, 147, 146);
    border-top-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
}

.tab:checked {
    background-color: rgb(50, 75, 75);
}

.unity-scroller__slider {
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
}

.unity-scroll-view__content-container--vertical {
    margin-right: 5px;
}
