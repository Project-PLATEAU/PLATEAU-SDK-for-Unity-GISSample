    border-top-right-radius: 2.5px;
    border-bottom-right-radius: 2.5px;
    border-bottom-left-radius: 2.5px;
    background-color: rgb(217, 217, 217);
}

.slider #unity-dragger {
    -unity-slice-left: 24;
    -unity-slice-right: 24;
    background-color: rgb(235, 235, 235);
    height: 24px;
    margin-top: -10px;
